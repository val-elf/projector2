{
    "openapi": "3.0.0",
    "info": {
        "title": "Projector Backend",
        "version": "1.0.0"
    },
    "components": {
        "securitySchemes": {
            "APIKeyHeader": {
                "type": "apiKey",
                "in": "header",
                "name": "Authorization"
            }
        },
        "security": {
            "APIKeyHeader": []
        },
        "schemas": {
            "TObjectId": {
                "type": "string"
            },
            "ICommonEntity": {
                "type": "object",
                "properties": {
                    "properties": {
                        "_id": {
                            "$ref": "#/components/schemas/TObjectId"
                        }
                    }
                },
                "description": "Common entity interface"
            },
            "EArtifactType": {
                "type": "string",
                "enum": [
                    "item",
                    "weapon",
                    "armor",
                    "spell",
                    "skill"
                ],
                "description": "Artifact entity type enum"
            },
            "EArtifactSubtype": {
                "type": "string",
                "enum": [
                    "steelArms",
                    "fireArms"
                ],
                "description": "Artifact entity subtype enum"
            },
            "IArtifact": {
                "type": "object",
                "properties": {
                    "properties": {
                        "preview": {
                            "$ref": "#/components/schemas/IPreview"
                        }
                    }
                },
                "required": [
                    "name",
                    "type",
                    "_coretype",
                    "_hash"
                ],
                "description": "Common artifact entity"
            },
            "IInitArtifact": {
                "type": "object",
                "properties": {
                    "properties": {
                        "preview": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "name",
                    "type"
                ],
                "description": "Update artifact entity client schema (create or update)"
            },
            "ICategory": {
                "type": "object",
                "properties": {
                    "properties": {
                        "_id": {
                            "$ref": "#/components/schemas/TObjectId"
                        }
                    }
                },
                "required": [
                    "name",
                    "description",
                    "_coretype",
                    "_hash"
                ],
                "description": "Category"
            },
            "IInitCategory": {
                "type": "object",
                "properties": {
                    "properties": {
                        "_id": {
                            "$ref": "#/components/schemas/TObjectId"
                        }
                    }
                },
                "required": [
                    "name",
                    "description"
                ],
                "description": "Initiated category item"
            },
            "IBaseCharacter": {
                "type": "object",
                "properties": {
                    "properties": {
                        "description": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "name",
                    "role",
                    "description"
                ],
                "description": "Base character interface"
            },


            "EDbObjectStatuses": {
                "type": "string",
                "enum": [
                    "normal",
                    "deleted"
                ],
                "description": "DbObject statuses"
            },
            "IPreview": {
                "type": "object",
                "properties": {
                    "properties": {
                        "_id": {
                            "$ref": "#/components/schemas/TObjectId"
                        }
                    }
                },
                "required": [
                    "type",
                    "hash",
                    "width",
                    "height"
                ],
                "description": "Preview interface for dbObjects"
            }

        }
    },
    "tags": [
